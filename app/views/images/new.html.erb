<h1>New Image</h1>
<%= form_tag "/images", method: :post do %> <!-- :method => :post -->
  <div class="form-group">
    Url: <input class="form-control" type='text' name="url">
  </div>
  <div class="form-group">
    Product ID: <input class="form-control" type='text' name="product_id" value="<%= %>">
  </div>
  <input type ="submit" value="Create Image">
<% end %>
<div><a href="/products">Back to Products</a></div>
